{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    },
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "indent": [
            true,
            "tabs"
        ],
        "interface-name": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-consecutive-blank-lines": false,
        "comment-format": false,
        "no-console": false,
        "no-trailing-whitespace": false,
        "trailing-comma": false
    }
}
